<%= javascript_tag do %>
  current_session_token = localStorage.getItem("session_token");
  if (current_session_token !== "<%= @sessions_tokens[0] %>" &&
      current_session_token !== "<%= @sessions_tokens[1] %>") {
      window.location = 'http://localhost:3000';
  }
<% end %>


<div class="main">
  <div class="messages-history" id="messages-history">
    <div id="messages">

    </div>
  </div>
</div>

<div class="footer">
  <div class="input-box">
    <input type="text" class="input-box_text"/>
  </div>

</div>